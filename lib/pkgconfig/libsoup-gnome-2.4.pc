prefix=/ucrt64
includedir=${prefix}/include
libdir=${prefix}/lib

exec_prefix=${prefix}

Name: libsoup
Description: A glib-based HTTP library
Version: 2.74.3
Requires: libsoup-2.4
Requires.private: glib-2.0 >=  2.58, gmodule-2.0 >=  2.58, gobject-2.0 >=  2.58, gio-2.0 >=  2.58, libxml-2.0, sqlite3, libpsl >=  0.20, libbrotlidec, zlib
Libs: -L${libdir} -lsoup-gnome-2.4
Libs.private: -lws2_32
Cflags: -I${includedir}/libsoup-gnome-2.4
